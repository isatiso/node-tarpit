{
    "name": "tarpit-root",
    "private": true,
    "author": "Cao Jiahang <sieglive@gmail.com>",
    "scripts": {
        "build": "rollup -c",
        "clean": "npm run clean-dest && npm run clean-coverage && lerna clean -y",
        "clean-coverage": "lerna exec 'rm -rf ./coverage ./.cache ./.nyc_output'",
        "clean-dest": "lerna exec 'rm -rf ./lib'"
    },
    "workspaces": [
        "modules/*",
        "packages/*",
        "projects/*",
        "supports/*"
    ],
    "dependencies": {
        "@tarpit/barbeque": "workspace:^0.5.24",
        "@tarpit/cli": "workspace:^0.5.24",
        "@tarpit/config": "workspace:^0.5.24",
        "@tarpit/content-type": "workspace:^0.5.24",
        "@tarpit/core": "workspace:^0.5.24",
        "@tarpit/cron": "workspace:^0.5.24",
        "@tarpit/dora": "workspace:^0.5.24",
        "@tarpit/error": "workspace:^0.5.24",
        "@tarpit/http": "workspace:^0.5.24",
        "@tarpit/judge": "workspace:^0.5.24",
        "@tarpit/rabbitmq": "workspace:^0.5.24",
        "@tarpit/schedule": "workspace:^0.5.24",
        "@tarpit/transformer": "workspace:^0.5.24",
        "@rollup/plugin-json": "^5.0.1",
        "@rollup/plugin-typescript": "^9.0.2",
        "@types/node": "^18.11.7",
        "@types/normalize-package-data": "^2.4.1",
        "builtin-modules": "^3.3.0",
        "fast-glob": "^3.2.12",
        "jsonwebtoken": "^8.5.1",
        "lerna": "^6.0.1",
        "normalize-package-data": "^5.0.0",
        "npm-check-updates": "^16.3.16",
        "rollup": "^3.2.3",
        "rollup-plugin-dts": "^5.0.0",
        "ts-node": "^10.9.1",
        "ttypescript": "^1.5.13",
        "typedoc": "^0.23.19",
        "typedoc-theme-hierarchy": "^3.0.0",
        "typescript": "^4.8.4"
    }
}
