{
    "name": "tarpit-root",
    "private": true,
    "author": "Cao Jiahang <sieglive@gmail.com>",
    "scripts": {
        "reset": "yarn clean && yarn && lerna run build",
        "clean": "npm run clean-docs && npm run clean-dest && npm run clean-coverage && lerna clean -y && rm -rf node_modules",
        "clean-docs": "rm -rf ./docs/_site && rm -rf ./docs/apis/*/",
        "clean-coverage": "lerna exec 'rm -rf ./coverage ./.cache ./.nyc_output'",
        "clean-dest": "lerna exec 'rm -rf ./lib'"
    },
    "workspaces": [
        "actions/*",
        "modules/*",
        "packages/*",
        "projects/*",
        "supports/*"
    ],
    "dependencies": {
        "@tarpit/barbeque": "^0.5.38",
        "@tarpit/cli": "^0.5.38",
        "@tarpit/config": "^0.5.38",
        "@tarpit/content-type": "^0.5.38",
        "@tarpit/core": "^0.5.38",
        "@tarpit/cron": "^0.5.38",
        "@tarpit/dora": "^0.5.38",
        "@tarpit/error": "^0.5.38",
        "@tarpit/http": "^0.5.38",
        "@tarpit/judge": "^0.5.38",
        "@tarpit/rabbitmq": "^0.5.38",
        "@tarpit/rollup": "^0.5.0",
        "@tarpit/schedule": "^0.5.38",
        "@tarpit/transformer": "^0.5.38",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-typescript": "^11.0.0",
        "@types/chai": "^4.3.4",
        "@types/chai-as-promised": "^7.1.5",
        "@types/chai-spies": "^1.0.3",
        "@types/mocha": "^10.0.1",
        "@types/node": "^18.14.0",
        "axios": "^1.3.4",
        "builtin-modules": "^3.3.0",
        "chai": "^4.3.7",
        "chai-as-promised": "^7.1.1",
        "chai-spies": "^1.0.0",
        "fast-glob": "^3.2.12",
        "jsonwebtoken": "^9.0.0",
        "lerna": "^6.5.1",
        "mocha": "^10.2.0",
        "normalize-package-data": "^5.0.0",
        "npm-check-updates": "^16.7.9",
        "nyc": "^15.1.0",
        "rollup": "^3.17.2",
        "rollup-plugin-dts": "^5.2.0",
        "ts-node": "^10.9.1",
        "ttypescript": "^1.5.15",
        "typedoc": "^0.23.25",
